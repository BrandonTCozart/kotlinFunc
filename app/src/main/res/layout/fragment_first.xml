<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragments.FirstFragment">

    <Button
        android:id="@+id/buttonToNotes"
        android:layout_width="412dp"
        android:layout_height="64dp"
        android:text="Notes"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.347" />

    <Button
        android:id="@+id/buttonToReminders"
        android:layout_width="413dp"
        android:layout_height="59dp"
        android:text="Reminders"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/buttonToNotes"
        app:layout_constraintStart_toStartOf="@+id/buttonToNotes"
        app:layout_constraintTop_toBottomOf="@+id/buttonToNotes"
        app:layout_constraintVertical_bias="0.09" />

    <Button
        android:id="@+id/buttonToTodos"
        android:layout_width="match_parent"
        android:layout_height="61dp"
        android:text="Todos"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/buttonToReminders"
        app:layout_constraintStart_toStartOf="@+id/buttonToReminders"
        app:layout_constraintTop_toBottomOf="@+id/buttonToReminders"
        app:layout_constraintVertical_bias="0.127" />
</androidx.constraintlayout.widget.ConstraintLayout>